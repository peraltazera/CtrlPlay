def soma(a, b):
    return a + b

def subtrai(a, b):
    return a - b

def multiplica(a, b):
    return a * b

def divide(a, b):
    if b == 0:
        raise ValueError("Não é possível dividir por zero")
    return a / b
    
def VerificarNumeroMaior(a, b):
    if a > b:
        return True
    else:
        return False
